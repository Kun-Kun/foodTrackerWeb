CREATE TABLE `food` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `gallery_id` int(11) NOT NULL,
  `title` varchar(255) NOT NULL,
  `description` MEDIUMTEXT NOT NULL,
  `portion_weight` float NOT NULL DEFAULT 0,
  `fats` float NOT NULL DEFAULT 0,
  `proteins` float NOT NULL DEFAULT 0,
  `carbohydrates` float NOT NULL DEFAULT 0,
  `kilocalories` float NOT NULL DEFAULT 0,
  `creator_id` int(11) DEFAULT NULL,
  `default_record` boolean DEFAULT true,
  `deleted` boolean DEFAULT false,
  `creation_date` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8mb4;
INSERT INTO `tracker`.`food` (`id`, `gallery_id`, `title`, `description`, `portion_weight`, `fats`, `proteins`, `carbohydrates`, `kilocalories`) VALUES ('1', '1', 'Борщ украинский', 'Это блюдо известно во всем мире и по праву считается истинной жемчужиной украинской кухни. История возникновения борща – в глубоком прошлом. Известно, что само название блюда походит от старославянского названия свеклы – бърщь. На сегодняшний день существует огромное количество видов данного блюда – во многом рецепт видоизменялся в зависимости от региона страны.', '250', '2.2', '1.1', '6.7', '49');
CREATE TABLE `daily_goal` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `eventDate` date NOT NULL,
  `diet_id` int(11) DEFAULT '1' NOT NULL,
  `kilocalories` float DEFAULT 0,

  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8mb4;

CREATE TABLE `eaten` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `daily_goal_id` int(11) NOT NULL,
  `food_id` int(11) NOT NULL,
  `repast_type` int(11) NOT NULL,
  `portion_weight` float NOT NULL DEFAULT 0,
  `fats` float NOT NULL DEFAULT 0,
  `proteins` float NOT NULL DEFAULT 0,
  `carbohydrates` float NOT NULL DEFAULT 0,
  `kilocalories` float NOT NULL DEFAULT 0,
  `change_date` date NOT NULL,

  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8mb4;
